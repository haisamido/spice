{
  "name": "sgp4-wasm",
  "version": "1.0.0",
  "description": "SGP4 satellite orbit propagation using NAIF CSPICE compiled to WebAssembly",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:ts": "tsc",
    "build": "npm run build:ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "start": "node dist/server.js",
    "clean": "rm -rf dist/*.js dist/*.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haisamido/sgp4-wasm.git"
  },
  "keywords": [
    "sgp4",
    "satellite",
    "orbit",
    "propagation",
    "tle",
    "two-line-element",
    "cspice",
    "spice",
    "naif",
    "jpl",
    "webassembly",
    "wasm",
    "aerospace",
    "space"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
