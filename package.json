{
  "name": "sgp4-wasm",
  "version": "1.0.0",
  "description": "SGP4 satellite orbit propagation using NAIF CSPICE compiled to WebAssembly",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:ts": "tsc",
    "build:copy": "cp lib/openapi.yaml dist/",
    "build": "npm run build:ts && npm run build:copy",
    "build:native": "cd src/native-addon && npx node-gyp configure && npx node-gyp build && mkdir -p ../../build/Release && cp build/Release/sgp4_native.node ../../build/Release/",
    "test": "vitest run",
    "test:watch": "vitest",
    "start": "node dist/server.js",
    "start:native": "node dist/server-native.js",
    "clean": "rm -rf dist/*.js dist/*.d.ts dist/*.yaml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haisamido/sgp4-wasm.git"
  },
  "keywords": [
    "sgp4",
    "satellite",
    "orbit",
    "propagation",
    "tle",
    "two-line-element",
    "cspice",
    "spice",
    "naif",
    "jpl",
    "webassembly",
    "wasm",
    "aerospace",
    "space"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "swagger-ui-express": "^5.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/yamljs": "^0.2.34",
    "node-addon-api": "^8.5.0",
    "node-gyp": "^11.5.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
